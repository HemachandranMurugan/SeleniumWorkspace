<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="Search School" parallel="tests">
	<listeners>
		<listener class-name="miniProjectSelenium.ExtentReportManager" />
	</listeners>
	<test name="Chrome_Test">
		<parameter name="browser" value="Chrome" />
		<classes>
			<class name="miniProjectSelenium.SearchSchoolTest" />
		</classes>
	</test>
	<test name="Edge_Test">
		<parameter name="browser" value="Edge" />
		<classes>
			<class name="miniProjectSelenium.SearchSchoolTest" />
		</classes>
	</test>
</suite> 
